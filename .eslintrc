{
    "rules": {
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "strict": [2, "global"],
        "no-multi-spaces": [0],
        "no-console": [0],
        "camelcase": [0]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "plugins": [
        "react"
    ]
}
